{
  "presets": ["next/babel"],
  "env": {
    "development": {
      "plugins": [
        ["transform-assets-import-to-string", {
          "baseDir": "/static",
          "extensions": [".jpg", ".png", ".mp4", ".css"]
        }]
      ]
    },
    "production": {
      "plugins": [
        ["transform-assets-import-to-string", {
          "baseDir": "/static",
          "baseUri": "https://assets.dennyku.com/hahow",
          "extensions": [".jpg", ".png", ".mp4", ".css"]
        }]
      ]
    }
  }
}